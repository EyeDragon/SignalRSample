@model IEnumerable<SignalRSample.Models.NotificationMessage>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MessageContent)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @(item.Id)
        </td>
        <td>
            @(item.Type ? "Notification" : "Alert")
        </td>
        <td>
            @(item.MessageContent)
        </td>
    </tr>
}

</table>
